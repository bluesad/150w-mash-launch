{
  "name": "mash-coming-soon-150w-studio",
  "description": "Mash from 150w studio",
  "version": "1.0.0",
  "author": "150w studio <support@150w.studio>",
  "homepage": "https://150w.studio",
  "dependencies": {
    "gatsby": "^2.32.13",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gh-pages": "^2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-particles-js": "^2.7.1"
  },
  "keywords": [
    "150w",
    "mash"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "jest",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d public"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^25.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.14.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ishida83/150w-mash-launch"
  }
}
